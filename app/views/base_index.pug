extends base_layout

block content
  div(class="container")
    div(class="table-responsive")
      table(class="table table-bordered table-hover")
        thead
          tr 
            th(scope="col") #
            th(scope="col") First
            th(scope="col") Last
            th(scope="col") Handle
        tbody 
          tr 
            th(scope="row") 1
            td Mark 
            td Otto 
            td @mdo
          tr 
            th(scope="row") 2
            td Jacob 
            td thornton 
            td @fef
          

